<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--图片圆角-->
    <dimen name="image_corner_radius_bas">@dimen/corner_radius_bas</dimen>

    <!--图片形状-->
    <style name="ImageShape">

    </style>

    <!--圆角图片-->
    <!--app:shapeAppearance="@style/ImageShape.Rounded"-->
    <style name="ImageShape.Rounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/image_corner_radius_bas</item>
    </style>

    <!--圆形图片-->
    <!--app:shapeAppearance="@style/ImageShape.Circle"-->
    <style name="ImageShape.Circle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>


</resources>